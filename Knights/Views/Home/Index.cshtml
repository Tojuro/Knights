@{
    ViewData["Title"] = "Home Page";
}



<table width="100%" height="100%">
    @for (int y = 0; y < 15; y++)
    {
    <tr>
        @for (int x = 0; x < 25; x++)
        {
            <td style="height: 25px; width: 25px; background-color: @Model.Board.Squares[y, x].StringColor">@Model.Board.Squares[y,x].Text</td>
        } &nbsp;
    </tr>
    }
</table>
